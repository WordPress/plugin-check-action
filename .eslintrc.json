{
  "extends": ["plugin:github/recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "camelcase": "off",
    "i18n-text/no-en": "off",
    "no-console": "off"
  },
  "env": {
    "node": true,
    "es6": true
  }
}
